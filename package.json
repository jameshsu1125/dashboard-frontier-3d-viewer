{
  "name": "template-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:serve": "concurrently \"npm run dev\" \"npm run serve\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rm": "rm -rf node_modules/ dist/ package-lock.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "NODE_ENV=production netlify deploy --dir=dist --prod",
    "serve": "NODE_ENV=local netlify dev --dir=dist",
    "copy": "cp -R ./node_modules/frontier-3d-viewer/lib/3d-viewer ./public"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@uidotdev/usehooks": "^2.4.1",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.2.0",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "frontier-3d-viewer": "^0.0.74",
    "lesca-fetcher": "^1.1.7",
    "lesca-local-storage": "^1.1.2",
    "lesca-react-capture-button": "^2.1.4",
    "lesca-react-onload": "^1.0.17",
    "lesca-use-tween": "^3.0.8",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "react-draft-wysiwyg": "^1.15.0",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.2.1",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.23.0",
    "serverless-http": "^3.2.0",
    "ts-toolbelt": "^9.6.0",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@netlify/functions": "^2.6.3",
    "@tailwindcss/typography": "^0.5.13",
    "@types/cors": "^2.8.17",
    "@types/draft-js": "^0.11.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.3.0",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "daisyui": "^4.11.1",
    "devcert": "^1.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "express": "^4.19.2",
    "gh-pages": "^6.1.1",
    "less": "^4.2.0",
    "mongoose": "^8.3.4",
    "msw": "^2.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-html": "^3.2.2"
